{
  "name": "avatar-practice-lab",
  "version": "1.0.0",
  "description": "AI Avatar Practice Lab - Voice-first conversation practice with HeyGen avatars",
  "type": "module",
  "scripts": {
    "dev": "tsx watch api/index.ts",
    "build": "npx vite build && (tsc -p tsconfig.build.json || true)",
    "start": "NODE_ENV=production node dist/api/index.js",
    "db:migrate": "drizzle-kit push",
    "db:migrate:sql": "tsx scripts/run-migrations.ts",
    "db:seed": "tsx scripts/run-seeds.ts",
    "db:setup": "npm run db:migrate:sql && npm run db:seed",
    "db:studio": "drizzle-kit studio",
    "db:seed-production": "tsx scripts/seed-production.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/lib-storage": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@heygen/streaming-avatar": "^2.0.0",
    "@neondatabase/serverless": "^0.9.0",
    "@openai/agents": "^0.0.10",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@radix-ui/react-toast": "^1.0.0",
    "@tanstack/react-query": "^5.0.0",
    "ahooks": "^3.9.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.0",
    "drizzle-orm": "^0.29.0",
    "drizzle-zod": "^0.5.0",
    "express": "^4.18.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.300.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.0.0",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.0.0",
    "react-media-recorder": "^1.6.0",
    "react-router-dom": "^6.0.0",
    "react-speech-recognition": "^3.10.0",
    "tailwind-merge": "^2.0.0",
    "uuid": "^9.0.0",
    "wouter": "^3.8.1",
    "zod": "^3.22.0"
